{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[allow]"
        ]
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-overflow-scrolling",
          "scrollbar-width",
          "scrollbar-color"
        ]
      }
    ]
  }
}